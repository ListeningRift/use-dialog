{
  "name": "use-dialog-vue3",
  "version": "0.0.2",
  "files": [
    "dist"
  ],
  "keywords": [
    "Vue3",
    "Modal",
    "Promise",
    "Hook"
  ],
  "description": "Vue3 Modal Promise Hook",
  "author": "ListeningRift",
  "license": "MIT",
  "homepage": "https://github.com/ListeningRift/use-dialog#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ListeningRift/use-dialog.git"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "rimraf dist && run-p type-check build-only",
    "build-only": "vite build",
    "type-check": "tsc",
    "lint": "eslint ./packages",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "peerDependencies": {
    "vue": "^3.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "^20.1.4",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.5",
    "typescript": "~5.0.4",
    "vite": "^4.3.9"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint --edit"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  }
}
